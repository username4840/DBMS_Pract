CREATE:-
mysql> create table collage(id int,name varchar(20),class varchar(10),division varchar(10)); Query OK, 0 rows affected (0.06 sec)

mysql> insert into liabrary(id,name,purpose,class,bookname)values(6,'sita','self- learning','TE','TOC'),(7,'yashodha','reading','BE','shakespear');
Query OK, 2 rows affected (0.01 sec) Records: 2 Duplicates: 0 Warnings: 0
mysql> select*from liabrary;
+	+	+	+	+	+
| id  | name	| purpose	| class | bookname  |
+	+	+	+	+	+
|	1 | Gauri	| books	| BE	| C++	|
|	2 | Pooja	| internet	| TE	| java	|
|	3 | Nilam	| reading	| TE	| NULL	|
|	4 | Ram	| internet	| FE	| C	|
|	5 | Sham	| bookreturn	| TE	| Python	|
|	6 | sita	| self-learning | TE	| TOC	|
|	7 | yashodha | reading	| BE	| shakespear |
+	+	+	+	+	+
7 rows in set (0.00 sec)
 
mysql> delete from liabrary where id>4; Query OK, 3 rows affected (0.01 sec)

mysql> insert into collage(id,name,class,division)values(5,'utkarsha','FE','A'),(6,'Rutuja','SE','A'),(7,'Shreya','BE','B'); Query OK, 3 rows affected (0.01 sec)
Records: 3 Duplicates: 0 Warning:0



mysql> select*from collage;
+	+	+	+	+
| id  | name	| class | division |
+	+	+	+	+
|	5 | utkarsha | FE	| A	|
|	6 | Rutuja  | SE	| A	|
|	7 | Shreya  | BE	| B	|
+	+	+	+	+
3 rows in set (0.00 sec)

UNION:-
mysql> select class from collage union select class from liabrary;
+	+
| class |
+	+
| FE	|
| SE	|
| BE	|
| TE	|
+	+
4 rows in set (0.01 sec)

mysql> select class from collage union all select class from liabrary;
+	+
| class |
+	+
| FE	|
| SE	|
| BE	|
| BE	|
| TE	|
| TE	|
| FE	|
+	+
7 rows in set (0.01 sec)

UNION ALL:-
mysql> select name from collage union all select name from liabrary;
+	+
 
| name	|
+	+
| utkarsha |
| Rutuja  |
| Shreya  |
| Gauri	|
| Pooja	|
| Nilam	|
| Ram	|
+	+

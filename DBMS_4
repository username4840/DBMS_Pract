SQL> declare 
  2  rno number(3):=&roll_no; 
  3  bname varchar2(10):='&name_of_book'; 
  4  no_day number(10); 
  5  issue_date date; 
  6  fine3amt number(5):=0; 
  7  begin 
  8  select date_of_issue into issue_date from library1 where roll_no=rno; 
  9  select sysdate-to_date(issue_date)days into no_day from dual; 
 10  if(no_day>=15 and no_day<=30)then 
 11  fine3amt:=no_day*5; 
 12  elsif no_day>30 then 
 13  fine3amt:=no_day*50; 
 14  end if; 
 15  update library1 set status='return' where roll_no=rno; 
 16  if fine3amt>0 then 
 17  insert into fine3 values(rno,sysdate,fine3amt); 
 18  end if; 
 19  EXCEPTION 
 20  WHEN no_data_found THEN 
 21  dbms_output.put_line('record not found'); 
 22  end; 
